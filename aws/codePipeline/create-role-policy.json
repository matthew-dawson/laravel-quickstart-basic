{
  "Version": "2012-10-17",
  "Statement": [
   {
      "Effect": "Allow",
      "Resource": "arn:aws:codestar-connections:eu-west-2:410539830770:*",
      "Action": "codestar-connections:UseConnection"
    },
    {
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::codepipeline-eu-west-2-laravel/*",
      "Action": "s3:*"
    },
    {
      "Effect": "Allow",
      "Resource": "arn:aws:codebuild:eu-west-2:410539830770:*",
      "Action": [
          "codebuild:StartBuild",
          "codebuild:BatchGetBuilds"
        ]
    }
  ]
}
